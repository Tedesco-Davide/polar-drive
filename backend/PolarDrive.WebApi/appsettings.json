{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Twilio": {
    "AccountSid": "ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
    "AuthToken": "your_auth_token_here",
    "PhoneNumber": "+393XXXXXXXXX",
    "WebhookUrl": "https://yourdomain.com/api/TwilioSms/webhook",
    "EnableSignatureValidation": true,
    "RateLimitPerMinute": 10,
    "AllowedPhoneNumbers": ["+393401234567", "+393987654321"]
  },
  "Tesla": {
    "RateLimits": {
      "RequestsPerHourPerVehicle": 200,
      "DelayBetweenVehiclesMs": 15000,
      "DelayBetweenTokenRefreshMs": 5000,
      "WakeUpDelayMs": 15000,
      "MaxRetries": 3,
      "RetryDelayMs": 30000,
      "CooldownAfterErrorMs": 300000
    },
    "Monitoring": {
      "EnableRateLimitTracking": true,
      "LogAllApiCalls": true,
      "AlertOnRateLimit": true,
      "AlertOnConsecutiveErrors": 3
    }
  }
}
